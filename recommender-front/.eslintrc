{
    "extends": "airbnb",
    "rules": {
        "react/prop-types": 0,
        "no-console": "off",
        "object-curly-newline": [
            "error",
            {
                "consistent": true
            }
        ]
    },
    "env": {
        "browser": true,
        "es6": true,
        "jest": true,
        "cypress/globals": true
    },
    "globals": {
        "Helmet": true,
        "HelmetProvider": true
    },
    "plugins": [
        "react",
        "jest",
        "cypress"
    ],
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    }
}